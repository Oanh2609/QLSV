@model IEnumerable<QLSV.Models.Lop>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/SinhVien/Views/Shared/_LayoutNV.cshtml";
}

<h2>Lớp</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaLop)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HeDaotao.TenHeDT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Khoa.TenKhoa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KhoaHoc.TenKhoaHoc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenLop)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.MaLop)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.HeDaotao.TenHeDT)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Khoa.TenKhoa)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.KhoaHoc.TenKhoaHoc)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TenLop)
    </td>

</tr>
}

</table>
